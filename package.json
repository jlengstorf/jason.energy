{
  "type": "module",
  "name": "jason.af",
  "private": true,
  "description": "the boopiest place on Earth",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "postcss --config commonjs-compat src/styles/*.css --dir .cache/styles/ && mkdir -p public/styles &&  cat .cache/styles/*.css > public/styles/global.css",
    "build": "toast incremental .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "breadbox": "breadbox --dest public/web_modules",
    "serve": "serve public"
  },
  "dependencies": {
    "@jlengstorf/get-share-image": "^0.7.1",
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/preact": "^1.6.19",
    "@sector/breadbox": "0.0.4",
    "@toastdotdev/mdx": "^0.1.3",
    "globby": "^11.0.1",
    "matter-js": "^0.14.2",
    "netlify-plugin-inline-source": "^1.0.4",
    "node-fetch": "^2.6.1",
    "patch-package": "^6.2.2",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "postcss-modules": "^3.2.2",
    "preact": "^10.5.5",
    "react-helmet": "^6.1.0",
    "rehype-identity-footnote-containers": "^1.0.0",
    "rehype-local-image-to-cloudinary": "^1.0.3",
    "remark-numbered-footnotes": "^2.0.0",
    "toast": "^0.3.30",
    "use-sound": "^1.0.2"
  },
  "devDependencies": {
    "prettier": "^2.1.2",
    "serve": "^11.3.2"
  },
  "snowpack": {
    "exclude": [
      "public/**",
      "commonjs-compat/**",
      "toast.js"
    ],
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      },
      "knownEntryPoints": [
        "preact",
        "preact/hooks"
      ]
    }
  }
}
